{
	"name": "@ctx-core/google",
	"version": "11.3.32",
	"description": "ctx-core google",
	"keywords": [
		"ctx-core",
		"google"
	],
	"homepage": "https://github.com/ctx-core/google#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/google/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/google.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./_types",
		"./analytics_google_script_",
		"./dataLayer__init",
		"./dataLayer__push",
		"./gtag_html_",
		"./gtm_body_script_html_",
		"./gtm_head_script_html_",
		"./gtm_script_html_",
		"./webfont_fout_html_"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build",
		"test": "pnpm test-unit && check-dts",
		"test-unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test-unit-coverage": "c8 pnpm test-unit"
	},
	"dependencies": {
		"@ctx-core/dom": "^15.1.29",
		"@ctx-core/env": "^17.4.32",
		"@ctx-core/error": "^12.4.28",
		"@ctx-core/object": "^31.1.10"
	},
	"devDependencies": {
		"@types/google.analytics": "^0.0.45",
		"c8": "^8.0.1",
		"check-dts": "^0.7.2",
		"eslint": "^8.54.0",
		"tsx": "^4.5.0",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false
}