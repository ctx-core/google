{
	"name": "@ctx-core/google",
	"version": "9.0.19",
	"description": "ctx-core google",
	"keywords": [
		"ctx-core",
		"google"
	],
	"homepage": "https://github.com/ctx-core/google#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/google/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/google/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"scripts": {
		"build": "npm run clean && npm run copy-components && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"copy-components": "cd src && find . | grep \\.svelte$ | awk '{print \"mkdir -p dirname \"$1\" && cp -f \"$1\" ../dist/\"$1}'",
		"exec": "$@",
		"prepublishOnly": "npm run build"
	},
	"dependencies": {
		"@ctx-core/dom": "workspace:^10.0.17",
		"@ctx-core/error": "workspace:^10.1.5",
		"@ctx-core/object": "workspace:^20.2.2"
	},
	"devDependencies": {
		"@ctx-core/ctx-core-package-tools": "workspace:^8.0.13",
		"@types/google.analytics": "^0.0.41",
		"svelte": "3.38.3",
		"typescript": "^4.3.4"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
