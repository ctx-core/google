{
	"name": "@ctx-core/google",
	"version": "9.0.0",
	"description": "ctx-core google",
	"keywords": [
		"ctx-core",
		"google"
	],
	"homepage": "https://github.com/ctx-core/google#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/google/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/google/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"main": "./src/index.js",
	"module": "./src/index.js",
	"types": "./src/index.d.ts",
	"scripts": {
		"build": "npm run clean && npm run compile && npm run symlink",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f dist/**/*.js && rm -f dist/**/*.d.ts && rm -f dist/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"symlink": "./node_modules/.bin/symlink_src_assets_to_dist .",
		"exec": "$@",
		"prepublishOnly": "npm run compile"
	},
	"dependencies": {
		"@ctx-core/dom": "workspace:^10.0.0",
		"@ctx-core/error": "workspace:^10.0.0",
		"@ctx-core/object": "workspace:^20.0.0"
	},
	"devDependencies": {
		"@ctx-core/ctx-core-package-tools": "workspace:^8.0.0",
		"@types/google.analytics": "^0.0.41",
		"svelte": "3.38.2",
		"typescript": "^4.3.2"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
